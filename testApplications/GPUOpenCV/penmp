    : SparseMatConstIterator(_m)
{}

template<typename _Tp> inline
SparseMatConstIterator_<_Tp>::SparseMatConstIterator_(const SparseMat* _m)
    : SparseMatConstIterator(_m)
{
    do { if(!!(_m->type() == traits::Type<_Tp>::value)) ; else cv::error( cv::Error::StsAssert, "_m->type() == traits::Type<_Tp>::value", __func__, "/usr/local/include/opencv2/core/mat.inl.hpp", 3162 ); } while(0);
}

