    : Mat(m, n / DataType<_Tp>::channels, traits::Type<_Tp>::value, (void*)&M)
{
    do { if(!!(n % DataType<_Tp>::channels == 0)) ; else cv::error( cv::Error::StsAssert, "n % DataType<_Tp>::channels == 0", __func__, "/usr/local/include/opencv2/core/mat.inl.hpp", 1594 ); } while(0);
    if( copyData )
        *this = clone();
}

