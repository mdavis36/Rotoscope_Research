    typedef Size_<typename DataType<_Tp>::work_type> work_type;
    typedef _Tp channel_type;

    enum { generic_type = 0,
           channels = 2,
           fmt = DataType<channel_type>::fmt + ((channels - 1) << 8)




         };

    typedef Vec<channel_type, channels> vec_type;
