# 286 "/usr/local/include/opencv2/core/utility.hpp" 3
 int64 getTickCount();
# 299 "/usr/local/include/opencv2/core/utility.hpp" 3
 double getTickFrequency();
# 328 "/usr/local/include/opencv2/core/utility.hpp" 3
class TickMeter
{
public:

    TickMeter()
    {
    reset();
    }




    void start()
    {
    startTime = cv::getTickCount();
    }




    void stop()
    {
    int64 time = cv::getTickCount();
    if (startTime == 0)
    return;
    ++counter;
    sumTime += (time - startTime);
    startTime = 0;
    }




    int64 getTimeTicks() const
    {
    return sumTime;
    }




    double getTimeMicro() const
    {
    return getTimeMilli()*1e3;
    }




    double getTimeMilli() const
    {
    return getTimeSec()*1e3;
    }




    double getTimeSec() const
    {
    return (double)getTimeTicks() / getTickFrequency();
    }




    int64 getCounter() const
    {
    return counter;
    }




    void reset()
    {
    startTime = 0;
    sumTime = 0;
    counter = 0;
    }

private:
    int64 counter;
    int64 sumTime;
    int64 startTime;
};
# 424 "/usr/local/include/opencv2/core/utility.hpp" 3
static inline
std::ostream& operator << (std::ostream& out, const TickMeter& tm)
{
    return out << tm.getTimeSec() << "sec";
}
